QFrame#loginTopbar, #loginWindow, #topbar, #sidebar, QWidget#mainControl, #mainGame, #mainScoreboard, #mainStart{
	background-color: rgb(210, 210, 220);
	border: 3px ridge rgb(150, 150, 150);
}

QLabel#loginLblName, #mainLblName{
	font-family: &quot;Quantico&quot;, &quot;Lucida Console&quot;, &quot;Arial&quot;;
	font-weight: bold;
	font-size: 50px;
}
QLineEdit{
	min-height: 30px;
}

QPushButton{
	min-height: 30px;
	border: 2px outset rgb(130, 130, 130);
	border-radius: 1px;
	background-color: 
		qlineargradient(
			spread:pad, x1:1, y1:0, x2:1, y2:1,
			stop:0.5 rgba(240, 240, 240, 255),
			stop:1 rgba(190, 190, 190, 255)
		);
}

QPushButton::Hover{
	border: 2px outset rgba(50, 50, 150, 200);
	background-color: 
		qlineargradient(
			spread:pad, x1:1, y1:0, x2:1, y2:1,
			stop:0.5 rgba(190, 190, 250, 255),
			stop:1 rgba(140, 140, 200, 255)
		);
}

QPushButton::Pressed{
	border: 2px inset rgb(130, 130, 130);
	border-radius: 1px;
	background-color: 
		qlineargradient(
			spread:pad, x1:1, y1:0, x2:1, y2:1,
			stop:0.5 rgba(240, 240, 240, 255),
			stop:1 rgba(190, 190, 190, 255)
		);
}
